
<%- include header %>
		<section class="main">
			<div class="main-header-title">
				<h1 class="row">javascript</h1></div>
			<div class="content row">
				<ul>
					<% posts.forEach(function (post, index) { %>
					<li class="main-list">
						<time><%= post.time.day %></time>
						<i class="sort"><span><%= post.pv %></span>浏览</i>
						<a class="detail" href=""><%= post.title %></a>
						<p><%- post.post %></p>
					</li>
					<% }) %>
				</ul>
			</div>
			
		</section>
	
